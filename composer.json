{
    "name": "ted81das/jitone-ai",
    "description": "jitone-ai is a powerful FilamentPHP plugin that integrates AI-powered features directly into your Filament forms.",
    "keywords": [
        "jiten14",
        "jitone-ai",
        "laravel",
        "laravel-package",
        "filament-plugin",
        "filamentphp",
        "ai-image-generation",
        "ai-content-generation"
    ],
    "homepage": "https://github.com/ted81das/jitone-ai",
    "support": {
        "issues": "https://github.com/ted81das/jitone-ai/issues",
        "source": "https://github.com/ted81das/jitone-ai"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Jitendriya Tripathy",
            "email": "jitendriya14@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.2",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "openai-php/laravel": "^0.10.1"
    },
    "require-dev": {
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Jiten14\\JitoneAi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jiten14\\JitoneAi\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jiten14\\JitoneAi\\JitoneAiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
